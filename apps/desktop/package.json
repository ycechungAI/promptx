{
  "name": "@promptx/desktop",
  "version": "1.27.7",
  "description": "PromptX Desktop - Local AI prompt management application",
  "author": {
    "name": "Deepractice",
    "email": "sean@deepracticex.com"
  },
  "homepage": "https://github.com/Deepractice/PromptX",
  "repository": {
    "type": "git",
    "url": "https://github.com/Deepractice/PromptX.git"
  },
  "type": "module",
  "main": "out/main/index.js",
  "engines": {
    "node": ">=18.17.0"
  },
  "scripts": {
    "postinstall": "node scripts/install-electron.cjs",
    "dev": "electron-vite dev",
    "build": "pnpm clean && electron-vite build",
    "build:force": "pnpm clean && electron-vite build",
    "preview": "electron-vite preview",
    "clean": "rimraf out dist release tsconfig.tsbuildinfo",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "package": "pnpm build && electron-builder",
    "package:mac": "pnpm build && electron-builder --config electron-builder-dev.yml",
    "package:mac:skip": "electron-builder --config electron-builder-dev.yml",
    "package:win": "pnpm build && electron-builder --win",
    "package:linux": "pnpm build && electron-builder --linux",
    "dist": "pnpm package:mac"
  },
  "dependencies": {
    "@promptx/config": "workspace:*",
    "@promptx/core": "workspace:*",
    "@promptx/mcp-server": "workspace:*",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/vite": "^4.1.16",
    "adm-zip": "^0.5.16",
    "auto-launch": "^5.0.5",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "electron-updater": "^6.6.2",
    "fs-extra": "^11.3.2",
    "i18next": "^23.15.1",
    "lucide-react": "^0.552.0",
    "next-themes": "^0.4.6",
    "react": "^18.2.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.0.2",
    "react-router-dom": "^6.8.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.4.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/auto-launch": "^5.0.4",
    "@types/fs-extra": "^11.0.4",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "autoprefixer": "^10.4.21",
    "electron": "32.2.7",
    "electron-builder": "^26.0.12",
    "electron-icon-builder": "^2.0.1",
    "electron-vite": "^2.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16"
  }
}